numbers = [21, 34, 54, 12]
print("list", numbers)
numbers.remove(34)
print("After remove", numbers)
numbers.insert(0, 1)
print("After Insertion", numbers)
numbers.append(32)
print("After append", numbers)
len(numbers)
print("Length", numbers)
numbers.pop(2)
print("After pop", numbers)
numbers.clear()
print("After clear", numbers)
